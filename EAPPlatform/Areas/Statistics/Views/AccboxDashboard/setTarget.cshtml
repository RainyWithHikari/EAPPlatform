<!DOCTYPE html>
<html>
<head>
<link rel="stylesheet" href="/layui/css/layui.css" media="all">
    <link rel="stylesheet" href="/font/iconfont.css">
    <link rel="stylesheet" href="/font-awesome/css/font-awesome.css">
    <link rel="shortcut icon" href="/favicon.ico" />
</head>
<body>
<form class="layui-form layui-form-pane" action="" style="width: 60%;margin: 0 auto;margin-top: 20px">
    <div class="layui-form-item">
        <label class="layui-form-label">Line</label>
        <div class="layui-input-block">
            <input id="line" class="layui-input" lay-filter="line" disabled>
        </div>
    </div>

   
    <div class="layui-form-item">
        <label class="layui-form-label">Target</label>
        <div class="layui-input-block">
            <input id="UPD" type="number" class="layui-input" lay-filter="UPD" placeholder="">
        </div>
    </div>

</form>
<script src="/echarts/echarts.min.js"></script>
    <script src="/layui/layui.js"></script>
    <script src="/bootstrap-5.1.3-dist/js/bootstrap.bundle.min.js"></script>
<script>

    layui.use(['jquery', 'form'], function () {
        var $ = layui.jquery;
        const form = layui.form;
        form.render();
        //回传给母页面的数据
        var retdata = {};
        

        window.callback = function () {
            retdata.eqptype = "ACCBOX";
            retdata.Target = $("#UPD").val();
            
            return JSON.stringify(retdata);
        };
        
        
        


    });
    
</script>

</body>
</html>