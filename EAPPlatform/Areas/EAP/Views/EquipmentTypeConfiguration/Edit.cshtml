<wt:container>
  <wt:form vm="@Model" url="/EAP/EquipmentTypeConfiguration/Edit" >
    <wt:row  items-per-row="ItemsPerRowEnum.Two">
      <wt:combobox field="Entity.TypeNameId"  item-url="/EAP/EquipmentTypeConfiguration/GetEquipmentTypes" empty-text="@Localizer["Sys.PleaseSelect"]" id="g6cc0dc66a8a44eec8efac2274070b6b0" multi-select="false"/>
